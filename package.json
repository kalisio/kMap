{
  "name": "@kalisio/kdk-map",
  "description": "Module to provide mapping capabiities for Kalisio applications and services",
  "version": "0.1.0",
  "homepage": "https://github.com/kalisio/kMap",
  "main": "lib/",
  "browser": {
    "lib/": "lib/client/"
  },
  "keywords": [
    "kalisio",
    "kalisio-plugin",
    "map"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/kMap.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/kMap/issues"
  },
  "engines": {
    "node": ">= 4.6.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:patch": "npm version patch && npm publish --access=public",
    "release:minor": "npm version minor && npm publish --access=public",
    "release:major": "npm version major && npm publish --access=public",
    "changelog": "github_changelog_generator -u kalisio -p kMap && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "compile": "shx rm -rf lib/ && babel -d lib/ src/ -s --copy-files",
    "watch:js": "babel --watch -d lib/ src/ --copy-files",
    "watch:vue": "cpx src/**/*.vue lib/ --watch",
    "watch": "npm-run-all --parallel watch:**",
    "lint": "standard src/**/*.js src/**/*.vue test/**/*.js --fix",
    "mocha": "cross-env NODE_CONFIG_DIR=./test/config/ mocha --opts mocha.opts",
    "coverage": "cross-env NODE_CONFIG_DIR=./test/config/ node node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "test": "npm run compile && npm run lint && npm run coverage",
    "start": "npm run compile && node lib"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "html"
    ],
    "sourceType": "module",
    "env": [
      "mocha"
    ],
    "globals": [
      "DEV",
      "PROD",
      "fetch",
      "cordova",
      "location"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/feathers": "^3.1.7",
    "cesium": "^1.38.0",
    "debug": "^3.1.0",
    "fs-extra": "^3.0.0",
    "leaflet": "^1.2.0",
    "leaflet-basemaps": "^0.2.2",
    "leaflet-fa-markers": "^0.0.5",
    "leaflet-filelayer": "^1.0.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-heatmap": "^1.0.0",
    "leaflet-measure": "^2.1.7",
    "leaflet.markercluster": "^1.0.5",
    "leaflet.vectorgrid": "^1.3.0",
    "loglevel": "^1.6.0",
    "node-geocoder": "^3.21.1",
    "quasar-extras": "0.0.8",
    "quasar-framework": "0.14.7",
    "togeojson": "^0.16.0",
    "vue": "2.5.17",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "chai-lint": "^0.1.1",
    "containerized": "^1.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "0.28.0",
    "eslint-plugin-html": "^3.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.1.1",
    "shx": "^0.2.2",
    "standard": "^10.0.2",
    "superagent": "^3.5.2",
    "vue-loader": "~12.2.2",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.5.2"
  },
  "peerDependencies": {}
}
